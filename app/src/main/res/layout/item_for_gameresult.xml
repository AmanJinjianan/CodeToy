<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <lecho.lib.hellocharts.view.ColumnChartView
        android:id="@+id/columnchart111"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5px"
        android:visibility="gone"
        android:background="@color/colorAccent"/>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_gameresult_stuname"
            android:layout_weight="3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="@drawable/border"
            android:layout_margin="1dp"
            android:gravity="center"
            android:text="TextView" />

        <TextView
            android:id="@+id/tv_gameresult_gender"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="@drawable/border"
            android:layout_margin="1dp"
            android:gravity="center"
            android:text="TextView" />

        <TextView
            android:id="@+id/tv_gameresult_gotcount"
            android:layout_weight="3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="@drawable/border"
            android:layout_margin="1dp"
            android:gravity="center"
            android:text="TextView" />

        <TextView
            android:id="@+id/tv_gameresult_losecount"
            android:layout_weight="3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="@drawable/border"
            android:layout_margin="1dp"
            android:gravity="center"
            android:text="TextView" />
        <TextView
            android:id="@+id/tv_gameresult_mark"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="@drawable/border"
            android:layout_margin="1dp"
            android:gravity="center"
            android:text="TextView" />

        <CheckBox
            android:id="@+id/cb_gameresult"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:focusableInTouchMode="false"/>
    </LinearLayout>


</FrameLayout>
